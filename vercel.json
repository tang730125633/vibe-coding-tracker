{
  "version": 2,
  "buildCommand": "cd output/frontend && npm install && npm run build",
  "outputDirectory": "output/frontend/dist",
  "installCommand": "cd output/backend && pip install -r requirements.txt",
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/output/frontend/dist/$1"
    }
  ],
